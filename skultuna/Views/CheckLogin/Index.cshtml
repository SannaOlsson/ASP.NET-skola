@model List<skultuna.Models.User>
<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" type="text/css" href="~/css/site.css">
    <title>Title of the document</title>
</head>
<body>
    <ul>
  		<li><a href="~/Home/Index">Skultuna</a></li>
  		<li><a href="~/Product/Index">Produkter</a></li>
  		<li style="float:right"><a href="~/User/Index">Logga In</a></li>
        <li style="float:right">@Html.ActionLink("Logga ut", "SignOut", "User")</li>
        <li style="float:right"><a href="~/Admin/Index">Admin</a></li>
	</ul>

    <h1>Användare</h1>

    <table border="1">
        <thead>
            <tr>
                <td>Användarnamn</td>
                <td>Lösenord</td>
            </tr>
        </thead>

        <!-- Loopar igenom alla användare och skriver ut de -->
        @foreach(var test in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => test.Username)</td>
                <td>@Html.DisplayFor(modelItem => test.Password)</td>
            </tr>
        }
    </table>
    <br />
</body>
</html>